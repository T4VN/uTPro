{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=(local);Database=uTPro;Integrated Security=true;TrustServerCertificate=true;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  },
  "uTPro": {
    "Backoffice": {
      "Enabled": true,
      "Domain": "bo.utpro.local"
    },
    "RememberLanguage": {
      "Enabled": true,
      "ListExludeRequestLanguage": {
        "Enabled": true,
        "Paths": [
        ]
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Smtp": {
          "From": "no.reply@t4vn.com",
          "Host": "smtp.gmail.com",
          "Port": 587,
          "Username": "no.reply@t4vn.com",
          "Password": "password",
          "SecureSocketOptions": "Auto",
          "DeliveryMethod": "Network"
        },
        "Id": "906dea88-5bb7-479d-b734-f6a5a1ab11df",
        "UseHttps": true,
        "SanitizeTinyMce": true,
        "UmbracoMediaPath": "~/uploads"
      },
      "ReservedPaths": "~/app_plugins/,~/install/,~/mini-profiler-resources/,~/umbraco/,~/error/",
      "Content": {
        "Notifications": {
          "Email": null
        },
        "LoginBackgroundImage": "../App_Plugins/assets/img/bkg-login.jpg",
        "LoginLogoImage": "../App_Plugins/assets/img/logo.svg",
        "LoginLogoImageAlternative": "../App_Plugins/assets/img/logo.svg",
        "BackOfficeLogo": "../App_Plugins/assets/img/logo.svg",
        "HideBackOfficeLogo": true,
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true,
          "KeepAllVersionsNewerThanDays": 7,
          "KeepLatestVersionPerDayForDays": 90
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Hosting": {
        "Debug": false
      },
      "Runtime": {
        "Mode": "Production"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Security": {
        "AllowConcurrentLogins": true
      }
    }
  }
}