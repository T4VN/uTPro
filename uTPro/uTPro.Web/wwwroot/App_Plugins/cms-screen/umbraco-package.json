{
    "alias": "login.extensions",
    "name": "Login extensions",
    "version": "1.0.0",
    "allowPublicAccess": true,
    "extensions": [
      {
        "type": "appEntryPoint",
        "alias": "uTProCMSScreen",
        "name": "uTPro CMS Screen",
        "js": "/App_Plugins/cms-screen/index.js",
        "weight": 1000
      },
      {
        "type": "localization",
        "alias": "Login.Localize.EnUS",
        "name": "English",
        "js": "/App_Plugins/cms-screen/lang/en-us.js",
        "weight": 1000,
        "meta": {
          "culture": "en-US"
        }
      }
    ]
}